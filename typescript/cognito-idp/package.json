  "bin": {
    "cognito-idp": "bin/cognito-idp.js"
  },
  "scripts": {
    "clean": "node clean.js",
    "build": "tsc && browserify web/index.ts -p [ tsify --noImplicitAny ]  > build/web/bundle.js && node build/scripts/copy-dist.js",
    "build-local": "tsc && browserify web/index.ts -p [ tsify --noImplicitAny ]  > web/bundle.js",
    "watch": "tsc -w",
    "test": "jest --group=unit",
    "cdk": "cdk",
    "unit-test": "jest --group=unit",
    "database-test": "jest --group=database",
    "cognito-test": "jest --group=cognito",
    "api-test": "jest --group=api",
    "handler-test": "jest --group=handler",
    "deploy": "cdk deploy --require-approval never"
  },
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@aws-cdk/aws-apigateway": "*",
    "@aws-cdk/aws-certificatemanager": "*",
    "@aws-cdk/aws-cloudfront": "*",
    "@aws-cdk/aws-cognito": "*",
    "@aws-cdk/aws-dynamodb": "*",
    "@aws-cdk/aws-route53-targets": "*",
    "@aws-cdk/aws-s3-deployment": "*",
    "@aws-cdk/cx-api": "*",
    "@aws-cdk/region-info": "*",
    "@types/aws-lambda": "^8.10.53",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^8.1.1",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/node": "10.17.5",
    "@types/qs": "^6.9.2",
    "@types/uuid": "^7.0.3",
    "aws-cdk": "^1.45.0",
    "browserify": "^16.5.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "jest": "^25.5.0",
    "jest-runner-groups": "^1.1.1",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.10.2",
    "tsify": "^4.0.2",
    "tslint": "^6.1.2",
